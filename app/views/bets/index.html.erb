    <h2><%= t('bets.index.title') %></h2>
    <table style="width:300px">
      <tr>
        <td><%= t('bets.index.raking') %></td>
        <td><%= t('bets.index.email') %></td>
        <td><%= t('bets.index.score') %></td>
        <td><%= t('bets.index.payment_status') %></td>
      </tr>
      <%= render partial: 'bets/bet', collection: @bets %>
    </table>
  </div>
</div>



